# PoolGuy CV Service - Environment Variables

# Flask Configuration
PORT=5000
FLASK_DEBUG=False
LOG_LEVEL=INFO

# Upload Configuration
UPLOAD_FOLDER=/tmp/poolguy-uploads

# Image Quality Thresholds
BRIGHTNESS_MIN=0.3
BRIGHTNESS_MAX=0.9
CONTRAST_MIN=0.5
FOCUS_MIN=0.5

# Processing Options
NORMALIZE_WHITE=true
MAX_IMAGE_SIZE_MB=10

# Request Timeout (seconds)
REQUEST_TIMEOUT=30
